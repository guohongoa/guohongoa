<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://m.ybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="department_relationship_info">

	<resultMap id="result" type="department_relationship_info">
		<result property="department_relationship_id" column="department_relationship_id" />
		<result property="department_relationship_upper_name" column="department_relationship_upper_name" />
		<result property="department_relationship_upper_id" column="department_relationship_upper_id" />
		<result property="department_relationship_downer_name" column="department_relationship_downer_name" />
		<result property="department_relationship_downer_id" column="department_relationship_downer_id" />
		<result property="department_relationship_upper_level" column="department_relationship_upper_level" />
		<result property="department_relationship_addtime" column="department_relationship_addtime" />
	</resultMap>

	<insert id="insert" parameterType="department_relationship_info">
		insert into
		department_relationship_info(department_relationship_id,department_relationship_upper_name,department_relationship_upper_id,department_relationship_downer_name,department_relationship_downer_id,department_relationship_upper_level,department_relationship_addtime)
		values(#{department_relationship_id},#{department_relationship_upper_name},#{department_relationship_upper_id},#{department_relationship_downer_name},#{department_relationship_downer_id},#{department_relationship_upper_level},#{department_relationship_addtime});
	</insert>
	
	<select id="select_by_upper_id" parameterType="int" resultMap="result">
		SELECT * FROM department_relationship_info WHERE department_relationship_upper_id = #{department_relationship_upper_id};
	</select>

</mapper>


