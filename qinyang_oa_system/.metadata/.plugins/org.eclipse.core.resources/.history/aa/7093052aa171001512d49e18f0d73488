<mapper namespace="system_user_info">
 
    <resultMap id="result" type="system_user_info">
        <result property="user_id" column="user_id"/>
        <result property="user_name" column="user_name"/>
        <result property="user_password" column="user_password"/>
           
    </resultMap>
 
    
 
    <insert id="insert" parameterType="system_user_info" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO persons (name) VALUES (#{name});
        INSERT INTO `hmkcode`.`system_user_info` ( `user_name`, `user_password`) VALUES (#{user_name},#{user_password});
    </insert>
 
</mapper>
