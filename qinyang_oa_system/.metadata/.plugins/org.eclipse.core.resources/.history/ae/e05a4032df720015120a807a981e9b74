<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://m.ybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="system_user_info">
 
    
 
    <insert id="insert" parameterType="system_user_info" >
        insert into system_user_info(user_name,user_password) values(#{user_name},#{user_password})
    </insert>
 
</mapper>

<mapper namespace="mail_info">
 
    
 
    <insert id="insert" parameterType="mail_info" >
        insert into mail_info(mail_sender,mail_receiver,mail_title,mail_content,mail_addtime) values(#{mail_sender},#{mail_receiver},#{mail_title},#{mail_content},#{,mail_addtime})
    </insert>
 
</mapper>
