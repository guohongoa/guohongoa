<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://m.ybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="system_user_info">
 
    <resultMap id="result" type="system_user_info">
        <result property="user_id" column="user_id"/>
        <result property="user_name" column="user_name"/>
        <result property="user_password" column="user_password"/>   
   </resultMap>
 
    <insert id="insert" parameterType="system_user_info"  useGeneratedKeys="true" keyProperty="user_id">
        insert into system_user_info(user_name,user_password) values(#{user_name},#{user_password})
    </insert>
    
    <select id="select_by_user_name" parameterType="String" resultMap="result">
        SELECT * FROM system_user_info WHERE user_name = #{user_name}
    </select>
    
 
</mapper>


